 2528|      0|					nx = pn - number_buf;
 2529|      0|					memcpy(word, number_buf, nx);
 2530|      0|					break;
 2531|      0|				}
 2532|      0|			}
 2533|      0|			pw = word;
 2534|      0|		}
 2535|      0|
 2536|      0|		for (n_digits = 0; iswdigit(word[n_digits]); n_digits++) // count consecutive digits
 2537|      0|			;
 2538|      0|
 2539|      0|		if (n_digits > 4) {
