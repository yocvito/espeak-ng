 1086|       |static void SetAmplitude(int length, unsigned char *amp_env, int value)
 1087|      0|{
 1088|      0|	if (wvoice == NULL)
 1089|      0|		return;
 1090|      0|
 1091|      0|	amp_ix = 0;
