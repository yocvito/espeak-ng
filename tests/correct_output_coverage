 2028|      0|				if (Lookup(tr, string, buf1) == 0)
 2029|      0|					break;
 2030|      0|				n_digits++;
 2031|      0|				strcat(ph_out, buf1);
 2032|      0|			}
 2033|      0|		}
 2034|      0|
 2035|      0|		while (IsDigit09(c = word[n_digits]) && (strlen(ph_out) < (N_WORD_PHONEMES - 10))) {
