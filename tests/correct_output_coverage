  486|      0|	int pfix;
  487|      0|	int n_chars;
  488|      0|	unsigned int dictionary_flags[2];
  489|      0|	unsigned int dictionary_flags2[2];
  490|      0|	int end_type = 0;
  491|      0|	int end_type1 = 0;
