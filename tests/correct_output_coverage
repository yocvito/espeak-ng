 1124|      0|					num_control |= 1;
 1125|      0|				} else
 1126|      0|					return 0;
 1127|      0|			}
 1128|      0|		} else
 1129|      0|			wtab[0].flags |= FLAG_ORDINAL;
 1130|      0|	}
 1131|      0|
 1132|      0|	tr->prev_dict_flags[0] = 0;
