  250|      0|	if ((Reverse4Bytes(pw[0]) != N_HASH_DICT) ||
  251|      0|	    (length <= 0) || (length > 0x8000000)) {
  252|      0|		fprintf(stderr, "Bad data: '%s' (%x length=%x)\n", fname, Reverse4Bytes(pw[0]), length);
  253|      0|		return 2;
  254|      0|	}
