  649|      0|		NULL, "rate", "volume", "pitch", "range"
  650|      0|	};
  651|      0|
  652|      0|	for (ix = 0; ix < (sizeof(tag_name)-1); ix++) {
  653|      0|		if (((c = xml_buf[ix]) == 0) || iswspace(c))
  654|      0|			break;
  655|      0|		tag_name[ix] = tolower((char)c);
