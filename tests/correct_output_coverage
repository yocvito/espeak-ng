 1319|      0|	int n_rules = 0;
 1320|      0|	int count = 0;
 1321|      0|	int different;
 1322|      0|	int wc;
 1323|      0|	int err_n_rules = 0;
 1324|      0|	const char *prev_rgroup_name;
 1325|      0|	unsigned int char_code;
