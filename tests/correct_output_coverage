 1415|      0|					} else {
 1416|      0|						if (control & 8) {
 1417|      0|							// is there a variant form of this number?
 1418|      0|							sprintf(string, "_%df", units);
 1419|      0|							found = Lookup(tr, string, ph_digits);
 1420|      0|						}
 1421|      0|						if ((is_ordinal) && ((tr->langopts.numbers & NUM_SWAP_TENS) == 0)) {
 1422|      0|							// ordinal
