 1439|      0|							}
 1440|      0|						}
 1441|      0|						if (found == 0) {
 1442|      0|							sprintf(string, "_%d", units);
 1443|      0|							Lookup(tr, string, ph_digits);
 1444|      0|						}
 1445|      0|					}
 1446|      0|				}
 1447|      0|			}
 1448|      0|		}
