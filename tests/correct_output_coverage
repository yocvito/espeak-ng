  122|      0|		return 1;
  123|      0|	case UCD_CATEGORY_Lt:
  124|      0|		return ucd_toupper(c) != c;
  125|      0|	case UCD_CATEGORY_Lm:
  126|      0|	case UCD_CATEGORY_Lo:
  127|      0|	case UCD_CATEGORY_Mn:
  128|      0|	case UCD_CATEGORY_Nl:
  129|      0|	case UCD_CATEGORY_So:
