 1996|      0|		if (!isspace2(*p))
 1997|      0|			break;
 1998|      0|	}
 1999|      0|	if (*p == 0) {
 2000|      0|		// No characters except spaces. This is not a sentence.
 2001|      0|		// Don't add this pause, just make up the previous pause to this value;
