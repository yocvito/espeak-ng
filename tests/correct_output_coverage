  874|      0|			if (value < 200)
  875|      0|				value = (value2 * 256) / (speed.pause_factor * 10);
  876|      0|
  877|      0|			if (terminator == 0)
  878|      0|				terminator = CLAUSE_NONE;
  879|      0|		}
