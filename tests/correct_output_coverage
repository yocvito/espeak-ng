  253|       |int utf8_nbytes(const char *buf)
  254|      0|{
  255|      0|	// Returns the number of bytes for the first UTF-8 character in buf
  256|      0|
  257|      0|	unsigned char c = (unsigned char)buf[0];
  258|      0|	if (c < 0x80)
  259|      0|		return 1;
  260|      0|	if (c < 0xe0)
