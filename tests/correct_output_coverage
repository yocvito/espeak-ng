  901|       |static int PlaySilence(int length, bool resume)
  902|      0|{
  903|      0|	static int n_samples;
  904|      0|	int value = 0;
  905|      0|
  906|      0|	nsamples = 0;
  907|      0|	samplecount = 0;
  908|      0|	wavephase = 0x7fffffff;
