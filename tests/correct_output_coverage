 2942|      0|				if (option_phonemes & espeakPHONEMES_TRACE) {
 2943|      0|					len = found - word1;
 2944|      0|					memcpy(word, word1, len); // include multiple matching words
 2945|      0|					word[len] = 0;
 2946|      0|					fprintf(f_trans, "Replace: %s  %s\n", word, *wordptr);
 2947|      0|				}
 2948|      0|			}
 2949|      0|
 2950|      0|			ph_out[0] = 0;
 2951|      0|			return 0;
 2952|      0|		}
 2953|      0|
 2954|      0|		return 1;
 2955|      0|	}
 2956|      0|
 2957|      0|	ph_out[0] = 0;
 2958|      0|	return 0;
