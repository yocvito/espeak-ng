 2928|      0|			*flags ^= FLAG_TEXTMODE;
 2929|      0|
 2930|      0|		if (*flags & FLAG_TEXTMODE) {
 2931|      0|			// the word translates to replacement text, not to phonemes
 2932|      0|
 2933|      0|			if (end_flags & FLAG_ALLOW_TEXTMODE) {
