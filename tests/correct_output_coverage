  496|      0|	char phonemes2[N_WORD_PHONEMES];
  497|      0|	char prefix_phonemes[N_WORD_PHONEMES];
  498|      0|	char unpron_phonemes[N_WORD_PHONEMES];
  499|      0|	char end_phonemes[N_WORD_PHONEMES];
  500|      0|	char end_phonemes2[N_WORD_PHONEMES];
