  142|       |	case UCD_CATEGORY_Cn:
  143|       |	case UCD_CATEGORY_Co:
  144|       |	case UCD_CATEGORY_Cs:
  145|       |	case UCD_CATEGORY_Ii:
  146|       |		return 0;
  147|       |	default:
  148|       |		return 1;
  149|       |	}
  150|       |}
