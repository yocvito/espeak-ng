 2592|      0|			}
 2593|      0|		} else {
 2594|      0|			pre_pause = 0;
 2595|      0|
 2596|      0|			dict_flags = TranslateWord2(tr, word, &words[ix], words[ix].pre_pause);
 2597|      0|
 2598|      0|			if (pre_pause > words[ix+1].pre_pause) {
 2599|      0|				words[ix+1].pre_pause = pre_pause;
 2600|      0|				pre_pause = 0;
 2601|      0|			}
