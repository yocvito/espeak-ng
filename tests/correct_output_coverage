  101|       |static int Reverse4Bytes(int word)
  102|      0|{
  103|      0|	// reverse the order of bytes from little-endian to big-endian
  104|       |#ifdef ARCH_BIG
  105|       |	int ix;
  106|       |	int word2 = 0;
  107|       |
  108|       |	for (ix = 0; ix <= 24; ix += 8) {
  109|       |		word2 = word2 << 8;
  110|       |		word2 |= (word >> ix) & 0xff;
  111|       |	}
  112|       |	return word2;
  113|       |#else
  114|       |	return word;
  115|      0|#endif
