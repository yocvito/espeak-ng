 1029|      0|		wcmdq[wcmdq_tail][0] = WCMD_MARKER + (type << 8);
 1030|      0|		wcmdq[wcmdq_tail][1] = (char_posn & 0xffffff) | (length << 24);
 1031|      0|		wcmdq[wcmdq_tail][2] = value;
 1032|      0|		WcmdqInc();
 1033|      0|	}
 1034|      0|}
