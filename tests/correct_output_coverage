  233|      0|	}
  234|      0|
  235|      0|	if ((tr->data_dictlist = malloc(size)) == NULL) {
  236|      0|		fclose(f);
  237|      0|		return 3;
  238|      0|	}
  239|      0|	size = fread(tr->data_dictlist, 1, size, f);
