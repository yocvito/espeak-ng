 1311|       |static espeak_ng_STATUS compile_dictrules(FILE *f_in, FILE *f_out, char *fname_temp, espeak_ng_ERROR_CONTEXT *context)
 1312|      0|{
 1313|      0|	char *prule;
 1314|      0|	unsigned char *p;
 1315|      0|	int ix;
