 1994|      0|				strcat(ph_out, buf1);
 1995|      0|				n_digits += decimal_count;
 1996|      0|			}
 1997|      0|			break;
 1998|      0|		case NUM_DFRACTION_1: // italian, say "hundredths" if leading zero
 1999|      0|		case NUM_DFRACTION_5: // hungarian, always say "tenths" etc.
