  885|      0|					value = 0xfff;
  886|      0|				terminator |= CLAUSE_PAUSE_LONG;
  887|      0|			}
  888|      0|			return terminator + value;
  889|      0|		}
  890|      0|		break;
  891|      0|	case SSML_SPEAK:
