  910|      0|	if (length == 0)
  911|      0|		return 0;
  912|      0|
  913|      0|	if (resume == false)
  914|      0|		n_samples = length;
  915|      0|
  916|      0|	while (n_samples-- > 0) {
  917|      0|		value = (echo_buf[echo_tail++] * echo_amp) >> 8;
  918|      0|
  919|      0|		if (echo_tail >= N_ECHO_BUF)
  920|      0|			echo_tail = 0;
