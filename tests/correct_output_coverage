  164|      0|		pw++;
  165|      0|		sign = -1;
  166|      0|	}
  167|      0|	value = (double)wcstod(pw, &tail);
  168|      0|	if (tail == pw) {
  169|      0|		// failed to find a number, return 100%
  170|      0|		*value_out = 100;
  171|      0|		return 2;
  172|      0|	}
  173|      0|
  174|      0|	if (*tail == '%') {
