  474|      0|	strcpy(tr->dictionary_name, name);
  475|      0|
  476|      0|	// convert name string into a word of up to 4 characters, for the switch()
  477|      0|	while (*name != 0)
  478|      0|		name2 = (name2 << 8) + *name++;
  479|      0|
  480|      0|	switch (name2)
  481|      0|	{
  482|      0|	case L('m', 'i'):
  483|      0|	case L('m', 'y'):
  484|      0|	case L4('p', 'i', 'q', 'd'): // piqd
  485|      0|	case L('p', 'y'):
