 1371|      0|		p++; // the first voice in the variants list is older
 1372|      0|
 1373|      0|	// add variants for the top voices
 1374|      0|	n_variants = 0;
 1375|      0|	for (ix = 0, ix2 = 0; ix < nv; ix++) {
 1376|      0|		vp = voices[ix];
 1377|      0|		// is the main voice the required gender?
 1378|      0|		skip = 0;
 1379|      0|
